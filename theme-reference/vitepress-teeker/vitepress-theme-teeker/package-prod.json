{
  "private": true,
  "name": "vitepress-theme-teeker",
  "version": "1.0.0",
  "description": "vitepress-theme-teeker",
  "type": "module",
  "author": {
    "name": "teeker",
    "email": "2456019588@qq.com",
    "url": "https://tianke99.cn"
  },
  "keywords": ["vitepress", "vite", "plugin", "permalink"],
  "license": "MIT",
  "homepage": "https://github.com/Kele-Bingtang/vitepress-theme-teeker/tree/master/plugins/vitepress-plugin-catalogue",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kele-Bingtang/vitepress-theme-teeker.git"
  },
  "bugs": {
    "url": "https://github.com/Kele-Bingtang/vitepress-theme-teeker/issues"
  },
  "main": "dist/lib/index.js",
  "module": "dist/es/index.mjs",
  "types": "dist/es/index.d.ts",
  "style": "index.css",
  "exports": {
    ".": {
      "types": "./dist/es/index.d.ts",
      "import": "./dist/es/index.mjs",
      "require": "./dist/lib/index.js"
    },
    "./es": {
      "types": "./dist/es/index.d.ts",
      "import": "./dist/es/index.mjs"
    },
    "./es/*.mjs": {
      "types": "./dist/es/*.d.ts",
      "import": "./dist/es/*.mjs"
    },
    "./es/*": {
      "types": ["./dist/es/*.d.ts", "./es/*/index.d.ts"],
      "import": "./dist/es/*.mjs"
    },
    "./lib": {
      "types": "./dist/lib/index.d.ts",
      "require": "./dist/lib/index.js"
    },
    "./lib/*.js": {
      "types": "./dist/lib/*.d.ts",
      "require": "./dist/lib/*.js"
    },
    "./lib/*": {
      "types": ["./dist/lib/*.d.ts", "./dist/lib/*/index.d.ts"],
      "require": "./dist/lib/*.js"
    },
    "./config": {
      "types": "./dist/es/config/index.d.ts",
      "import": "./dist/es/config/index.mjs"
    },
    "./*": "./dist/*"
  },
  "scripts": {
    "stub": "unbuild --stub"
  },
  "dependencies": {
    "vitepress-plugin-catalogue": "workspace:*",
    "vitepress-plugin-doc-analysis": "workspace:*",
    "vitepress-plugin-md-h1": "workspace:*",
    "vitepress-plugin-permalink": "workspace:*",
    "vitepress-plugin-sidebar-resolve": "workspace:*",
    "vitepress-plugin-file-content-loader": "workspace:*",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@giscus/vue": "^3.1.1",
    "@waline/client": "^3.5.1",
    "element-plus": "^2.9.3",
    "sass": "^1.83.1",
    "unbuild": "^3.2.0",
    "vite": "^6.0.7",
    "vitepress": "^1.6.3"
  },
  "peerDependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "element-plus": "^2.9.3",
    "vite": "^6.0.7",
    "vitepress": "^1.6.3"
  },
  "browserslist": ["> 1%", "not ie 11", "not op_mini all"]
}
